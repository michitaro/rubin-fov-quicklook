        "/api/visits": {
            "get": {
                "summary": "List Visits",
                "operationId": "list_visits",
                "parameters": [
                    {
                        "name": "exposure",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ],
                            "title": "Exposure"
                        }
                    },
                    {
                        "name": "day_obs",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "anyOf": [
                                {
                                    "type": "integer"
                                },
                                {
                                    "type": "null"
                                }
                            ],
                            "title": "Day Obs"
                        }
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "maximum": 10000,
                            "default": 1000,
                            "title": "Limit"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/VisitListEntry"
                                    },
                                    "title": "Response List Visits Api Visits Get"
                                }
                            }
                        }
                    },
                    "422": {
